{
  "name": "faster-digital-garden",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch\" \"node server\"",
    "build": "npm run build:css && npm run build:client && npm run build:server",
    "build:client": "exit 0; vite build src/entry-client.tsx --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "serve": "NODE_ENV=production node server",
    "start": "npm run build && npm run serve",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.99.0",
    "@tanstack/router-devtools": "^1.99.0",
    "@tanstack/router-plugin": "^1.99.3",
    "@tanstack/start": "^1.99.3",
    "autoprefixer": "^10.4.20",
    "get-port": "^7.1.0",
    "postcss": "^8.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "redaxios": "^0.5.1",
    "spotify-web-api-node": "^5.0.2",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/generator": "^7.26.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/express": "^4.17.21",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "compression": "^1.7.5",
    "concurrently": "^9.1.2",
    "express": "^4.21.2",
    "isbot": "^5.1.22",
    "node-fetch": "^3.3.2",
    "serve-static": "^1.16.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.11"
  }
}
